<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>Zebble.PushNotification</id>
    <version>4.0.175</version>
    <title>Push Notification (Zebble)</title>
    <projectUrl>https://github.com/Geeksltd/Zebble.PushNotification</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/Geeksltd/Zebble.PushNotification/master/Shared/Icon.png</iconUrl>
    <copyright>Copyright © 2018 Geeks Ltd - All rights reserved.</copyright>
    <authors>Geeks Ltd</authors>
    <description>Cross platform push notification plugin for Zebble apps.</description>
    <dependencies>
      <group targetFramework="MonoAndroid10">
        <dependency id="Xamarin.Firebase.Messaging" version="42.1021.1" />
        <dependency id="Zebble.LocalNotification" version="1.0.102" />
      </group>
      <group targetFramework="Xamarin.iOS10">
        <dependency id="Zebble.LocalNotification" version="1.0.102" />
      </group>
      <group targetFramework="UAP10">
        <dependency id="Zebble.LocalNotification" version="1.0.102" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="..\..\lib\Zebble.PushNotification.iOS.*" target="lib\Xamarin.iOS10\" />
    <file src="..\..\lib\Zebble.PushNotification.Android.*" target="lib\MonoAndroid10\" />
    <file src="..\..\lib\Zebble.PushNotification.UWP.*" target="lib\UAP10\" />
  </files>
</package>